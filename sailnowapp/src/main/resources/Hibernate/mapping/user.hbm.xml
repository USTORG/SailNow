<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.sailnow.models">
    <class name="UserModel" table="User">
      <id name="email" type="string" column="email"/>
      
        <!--<property name="name" column="NAME" />-->
 
        <set name="packages" table="UserPackage" cascade="all">
            <key column="email" not-null="true" />
            <many-to-many column="name" class="PackageModel" unique="true"/>
        </set>
    </class> 
</hibernate-mapping>