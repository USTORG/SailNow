<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>AuthenticationFilter</filter-name>
  	<display-name>AuthenticationFilter</display-name>
  	<description></description>
  	<filter-class>com.sailnow.filters.AuthenticationFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>AuthenticationFilter</filter-name>
  	<url-pattern>/protected/*</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>SignIn</servlet-name>
  	<display-name>SignIn</display-name>
  	<description></description>
  	<servlet-class>com.sailnow.servlets.SignInServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>oauth2callback</servlet-name>
  	<display-name>oauth2callback</display-name>
  	<description></description>
  	<servlet-class>com.sailnow.servlets.OAuthCodeCallbackHandlerServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Logout</servlet-name>
  	<display-name>Logout</display-name>
  	<description></description>
  	<servlet-class>com.sailnow.servlets.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ItemService</servlet-name>
  	<display-name>ItemService</display-name>
  	<description></description>
  	<servlet-class>com.sailnow.servlets.ItemServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SignIn</servlet-name>
  	<url-pattern>/SignIn</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>oauth2callback</servlet-name>
  	<url-pattern>/oauth2callback</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Logout</servlet-name>
  	<url-pattern>/Logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ItemService</servlet-name>
  	<url-pattern>/ItemService</url-pattern>
  </servlet-mapping>
</web-app>
